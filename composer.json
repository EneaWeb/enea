{
    "name": "laravel/laravel",
    "description": "The Laravel Framework.",
    "keywords": ["framework", "laravel"],
    "license": "MIT",
    "type": "project",
    "require": {
        "php": ">=5.6.4",
        "arcanedev/localization": "^1.0",
        "barryvdh/laravel-dompdf": "^0.7.1",
        "egeloen/google-map": "dev-master",
        "geocoder-php/geocoder-extra": "1.4.0",
        "gloudemans/shoppingcart": "^2.3",
        "intervention/image": "^2.3",
        "laravel/framework": "5.4.*",
        "laravel/tinker": "~1.0",
        "laravelcollective/html": "5.4.x-dev",
        "league/flysystem-aws-s3-v3": "^1.0",
        "maatwebsite/excel": "^2.1",
        "spatie/laravel-permission": "^1.7",
        "vinkla/algolia": "^2.5",
        "willdurand/geocoder": "2.8.2"
    },
    "require-dev": {
        "fzaninotto/faker": "~1.4",
        "mockery/mockery": "0.9.*",
        "phpunit/phpunit": "~5.0"
    },
    "autoload": {
        "classmap": [
            "database"
        ],
        "psr-4": {
            "App\\": "app/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "scripts": {
        "post-root-package-install": [
            "php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "php artisan key:generate"
        ],
        "post-install-cmd": [
            "Illuminate\\Foundation\\ComposerScripts::postInstall",
            "php artisan optimize"
        ],
        "post-update-cmd": [
            "Illuminate\\Foundation\\ComposerScripts::postUpdate",
            "php artisan optimize"
        ]
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true
    },
    
    "repositories": {
        "laravelcollective": {
            "type": "vcs",
            "url": "https://github.com/maddhatter/html.git"
        },
        "egeloen": {
            "type": "git",
            "url": "https://github.com/EneaWeb/ivory-google-map.git"
        }
    }
}
